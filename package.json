{
  "name": "@truto/langchain-toolset",
  "private": false,
  "publishConfig": {
    "access": "public"
  },
  "version": "0.1.0",
  "description": "SuperAI toolset for TypeScript. Works in Langchain.js.",
  "repository": "https://github.com/trutohq/truto-langchainjs-toolset.git",
  "source": "src/index.ts",
  "main": "dist/main.cjs",
  "module": "dist/module.js",
  "types": "dist/types.d.ts",
  "files": [
    "dist"
  ],
  "exports": {
    ".": {
      "require": "./dist/main.cjs",
      "import": "./dist/module.js"
    }
  },
  "targets": {
    "main": {
      "includeNodeModules": [
        "lodash-es"
      ],
      "isLibrary": true
    },
    "module": {
      "includeNodeModules": [
        "lodash-es"
      ],
      "isLibrary": true
    }
  },
  "type": "module",
  "license": "MIT",
  "volta": {
    "node": "22.14.0",
    "yarn": "1.22.19"
  },
  "devDependencies": {
    "@parcel/packager-ts": "2.8.3",
    "@parcel/transformer-typescript-types": "2.8.3",
    "@types/jest": "29.4.0",
    "@types/lodash-es": "4.17.7",
    "@types/node": "18.14.6",
    "@typescript-eslint/eslint-plugin": "5.46.1",
    "@typescript-eslint/parser": "5.46.1",
    "@langchain/openai": "0.4.4",
    "eslint": "8.29.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-plugin-prettier": "4.2.1",
    "ora": "8.2.0",
    "parcel": "2.14.2",
    "prettier": "2.8.4",
    "ts-node": "10.9.1",
    "typescript": "4.9.5"
  },
  "dependencies": {
    "@truto/truto-ts-sdk": "2.0.2",
    "@langchain/core": "0.3.42",
    "lodash-es": "4.17.21"
  },
  "scripts": {
    "dev": "parcel watch",
    "build": "rm -rf dist && parcel build",
    "check": "tsc --noEmit",
    "prepublishOnly": "yarn build"
  },
  "resolutions": {
    "word-wrap": "npm:@aashutoshrathi/word-wrap@1.2.5",
    "semver": "npm:semver@7.5.4"
  }
}
